{
  "name": "protected-app-demo",
  "version": "1.0.0",
  "description": "Web-app to demonstrate JWT authorization",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "ignore": [
      "dist/"
    ],
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "scripts": {
    "test": "nyc mocha --harmony --compilers js:babel-core/register test/**/*.test.js",
    "babel:transpile": "babel src --out-dir lib",
    "start": "cp index.html public/index.html && babel src --out-dir lib && webpack && node lib/index.js"
  },
  "keywords": [
    "react",
    "redux",
    "JSON",
    "web",
    "tokens"
  ],
  "author": "Christian Wilkerson",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.0",
    "eslint-plugin-react": "^7.1.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "lolex": "^1.6.0",
    "mocha": "^3.5.0",
    "mock-local-storage": "^1.0.4",
    "nyc": "^11.1.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.1",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^2.12.0",
    "supertest": "^3.0.0",
    "webpack": "^3.4.1"
  }
}
